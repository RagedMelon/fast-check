/* components/inspection/index/index.wxss */

.result {
    background-color: #fff;
    width: 622rpx;
    height: 400rpx;
    margin: 50rpx auto 0;
    padding: 0 32rpx;
    border-radius: 24rpx;
}

.row {
    display: flex;
    justify-content: space-between;
    height: 72rpx;
    line-height: 72rpx;
    border-bottom: 2rpx solid #FAFAFA;
    color: rgba(51, 51, 51, 1);
    font-size: 28rpx;
    font-family: Alibaba-PuHuiTi-R;
}

.tag {
    color: rgba(128, 128, 128, 1);
}

.positive {
    color: red
}

.negative {
    color: green;
}