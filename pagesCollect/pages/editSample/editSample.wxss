    font-family: Alibaba-PuHuiTi-R;
    position: relative;
}

.title .pic {
    width: 18rpx;
    height: 12rpx;
    margin-left: 18rpx;
}

.title-option {
    position: absolute;
    top: 50rpx;
    right: -36rpx;
    /* padding: 18rpx; */
    background-color: #fff;
    text-align: center;
    border: 2rpx solid #f2f3f5;
    border-radius: 24rpx;
    z-index: 5;
    height: 400rpx;
