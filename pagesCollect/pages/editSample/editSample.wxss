    border-radius: 24rpx;
    z-index: 5;
    min-width: 8em;
    max-width: 24em;
}

.option {
    padding: 18rpx;
    height: 88rpx;
    line-height: 88rpx;
    border-bottom: 2rpx solid #FAFAFA
}

.shop-num {
    display: flex;
    justify-content: space-between;
}

.btns {
    height: 80rpx;
    display: flex;
    width: 686rpx;
    justify-content: space-between;
    margin: 32rpx auto 80rpx;
    font-size: 28rpx;
}

.btns {
    height: 80rpx;
    display: flex;
    width: 686rpx;
    justify-content: space-between;
    margin: 32rpx auto 50rpx;
    font-size: 28rpx;
}

.draft {
    width: 326rpx;
    height: 80rpx;
    line-height: 80rpx;
    border-radius: 40rpx;
    background-color: rgba(255, 255, 255, 1);
    border: 2rpx solid rgba(242, 242, 242, 1);
    color: rgba(255, 197, 26, 1);
    text-align: center;
}

.submit {
    width: 326rpx;
    height: 80rpx;
    line-height: 80rpx;
    border-radius: 40rpx;
    background-color: rgba(255, 197, 26, 1);
    box-shadow: 0 20rpx 20rpx 0 rgba(255, 197, 26, 0.2);
    color: white;
    text-align: center;
}

.owner-info {
    display: flex;
    justify-content: space-between;
    width: 686rpx;
    margin: 32rpx auto;
}

.icon-owner {
    width: 36rpx;
    height: 36rpx;
    vertical-align: middle;
    margin-right: 16rpx;
}

.title {
    font-size: 28rpx;
    font-family: Alibaba-PuHuiTi-R;
}

.switch {
    color: rgba(204, 204, 204, 1);
    font-size: 28rpx;
    font-family: Alibaba-PuHuiTi-R;
}

.switch .up {
    width: 18rpx;
    height: 12rpx;
    vertical-align: middle;
    margin-left: 18rpx;
    /* transform: rotate(180deg); */
    transition: All 0.3s ease-in-out;
}

.switch .down {
    width: 18rpx;
    height: 12rpx;
    vertical-align: middle;
    margin-left: 18rpx;
    transform: rotate(180deg);
    transition: All 0.3s ease-in-out;
}


